package atrapa_la_fruta;

public class frutamagica extends fruta {
	
	//Repito los atributos en las clases para evitar cualquier posible confusión con los de otras clases
	private String nombre;
	private int puntos;
	protected int posicionX;
	protected int posicionY;
	protected int alto;
	protected int ancho;
	protected String imagen;
	protected char simbolo;
	enum efectos{desenfocar, correr, ralentizar};

	public frutamagica(int posicionX, int posicionY, int ancho, int alto, char simbolo, String imagen, efectos efecto) {
		super(posicionX, posicionY, ancho, alto, simbolo, imagen);
		
		
	}

	

}
